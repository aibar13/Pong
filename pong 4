int a = 500;
 
 void setup(){
 size(800,600);
 }
void draw(){
 background(0);
 stroke(255,255,255);
 line(400,0,400,600);
  
noStroke();
 rect(50,a,20,90);
 if(keyPressed && (key==CODED))
 if(keyCode== UP){
  if(a > 0){
  a=a-5;}
}
 else if(keyCode== DOWN){
   if(a < 600-90){
  a=a+5;}
 }
 
rect(750,a,20,90);
 if(keyPressed && (key==CODED))
 if(keyCode== 'w'){
  a=a-5;}
 else if(keyCode== 's'){
  a=a+5;}
}
