 size(800,600);
}
void draw(){
 background(0);
 stroke(255,0,0);
 line(400,0,400,600);
  
noStroke();
 rect(50,y,20,90);
 if(keyPressed && (key==CODED))
 if(keyCode== UP){
  if(y>0){
  y=y-4;}
}
 else if(keyCode== DOWN){
   if(y<600){
  y=y+4;}
 }
 
rect(750,y,20,90);
 if(keyPressed && (key==CODED))
 if(keyCode== 'w'){
  y=y-4;}
 else if(keyCode== 's'){
  y=y+4;}
}
